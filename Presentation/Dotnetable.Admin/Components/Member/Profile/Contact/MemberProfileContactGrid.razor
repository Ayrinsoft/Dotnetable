<Dotnetable.Admin.Sahred.FormControls.DNGridView GridHeaderParams="_gridHeaderParams">
    @if (AddressList != null && AddressList.Count > 0)
    {
        foreach (var j in AddressList)
        {
            <tr>
                <td>@j.CityName</td>
                <td>@j.Address</td>
                <td>@j.CellphoneNumber</td>
                <td>
                    <MudButtonGroup Color="Color.Primary" Variant="Variant.Outlined">
                        <MudButton>@_loc["_Management"]</MudButton>
                        <MudMenu Icon="@Icons.Material.Filled.ArrowDropDown">
                            <MudMenuItem Icon="@Icons.Material.Filled.Update" OnClick="@(() => UpdateAddress(j))">@_loc["_Update"]</MudMenuItem>
                            <MudMenuItem Icon="@Icons.Material.Filled.Remove" OnClick="@(() => RemoveAddress(j.MemberContactID))">@_loc["_Remove"]</MudMenuItem>
                        </MudMenu>
                    </MudButtonGroup>
                </td>
            </tr>
        }
    }
</Dotnetable.Admin.Sahred.FormControls.DNGridView>